!doctype html
html(){
	head(){
		title(){@SiteName}
        meta({name:"keywords", content:"@Keywords"});
        meta({name:"description", content:"@Description"});
        !section header
	}
	body(){
		ul({class:"list"}, collection){
			@foreach(var model in sql("SELECT Name FROM data_Demos")){
				li({class:"list-item"}){
					span(){@model.Name}
					@if(model.Count>0){
						ul.({class:"list"}, collection);
					}
				}
			}
		}
	}
}